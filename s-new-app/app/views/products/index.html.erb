<p id="notice"><%= notice %></p>

<h1>Products</h1>


<div class="container-fluid">

  <div class="row">
    <% cache (cache_key_for_products) do %>
      <% @products.each do |product| %>
        <%= render product %>
      <% end %>
    <% end %>
  </div><!--end of row-->
</div>


<%= link_to 'New Product', new_product_path %>
